{
  "name": "motabea-school-management",
  "version": "1.0.0",
  "description": "MOTABEA - Comprehensive School Management System",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "dev:frontend": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "start": "start-project.bat",
    "start:all": "start-project.bat",
    "start:backend": "start-server.bat",
    "start:simple": "node quick-start.js",
    "start:advanced": "node start-dev.js",
    "start:motabea": "start-motabea.bat",
    "start:old": "concurrently \"npm run server\" \"npm run dev:frontend\"",
    "clean": "taskkill /F /IM node.exe /T 2>nul || echo 'No processes to kill'",
    "restart": "npm run clean && timeout /t 3 /nobreak >nul && npm start",
    "health": "node health-check-advanced.js"
  },
